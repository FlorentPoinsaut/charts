{
  "phases": {
    "package": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/bitnami/jaeger"
        }
      },
      "actions": [
        {
          "action_id": "helm-package"
        },
        {
          "action_id": "helm-lint"
        }
      ]
    },
    "verify": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/bitnami/jaeger"
        },
        "runtime_parameters": "cXVlcnk6CiAgc2VydmljZToKICAgIHBvcnRzOgogICAgICBhcGk6IDgw",
        "target_platform": {
          "target_platform_id": "{VIB_ENV_TARGET_PLATFORM}",
          "size": {
            "name": "M4"
          }
        }
      },
      "actions": [
        {
          "action_id": "health-check",
          "params": {
            "endpoint": "lb-jaeger-query-frontend-http??",
            "app_protocol": "HTTP"
          }
        },
        {
          "action_id": "cypress",
          "params": {
            "resources": {
              "path": "/.vib/jaeger/cypress"
            },
            "endpoint": "lb-jaeger-compactor-http",
            "app_protocol": "HTTP",
            "env": {
              "compactorReplicaCount": "2",
              "distributorReplicaCount": "1",
              "ingesterReplicaCount": "1",
              "querierReplicaCount": "1",
              "gossipRingPort": "7946"
            }
          }
        }
      ]
    }
  }
}
